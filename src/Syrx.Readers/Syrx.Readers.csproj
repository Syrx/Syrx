<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.1.0</Version>
    <Authors>@sextondjc</Authors>
    <Company />
    <Description>Interface for building Syrx command readers used within the Syrx framework.</Description>
    <PackageProjectUrl>https://github.com/Syrx</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Syrx/Syrx</RepositoryUrl>
    <PackageReleaseNotes>Updated to .NET standard 2.1.</PackageReleaseNotes>
    <Copyright>https://github.com/Syrx</Copyright>
    <PackageTags>syrx</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Syrx.Settings\Syrx.Settings.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
