<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.2.0</Version>
    <Authors>@sextondjc</Authors>
    <Company />
    <Description>A collection of interfaces for building configuration sources and types for use in the Syrx framework.</Description>
    <PackageProjectUrl>https://github.com/Syrx</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Syrx/Syrx</RepositoryUrl>
    <PackageReleaseNotes>Updated to .NET standard 2.1.</PackageReleaseNotes>
    <Copyright>https://github.com/Syrx</Copyright>
    <PackageTags>syrx</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
